{
  "name": "gatsby-starter-hello-world",
  "description": "Gatsby hello world starter",
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop -H 0.0.0.0",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "lint": "eslint 'src/**/*.js'",
    "lintfix": "eslint --fix 'src/**/*.js' && prettier --write 'src/**/*.js'",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "test": "cypress run",
    "ci": "start-server-and-test start http://localhost:8000 test"
  },
  "dependencies": {
    "gatsby": "5.11.0",
    "gatsby-plugin-feed": "5.11.0",
    "gatsby-plugin-google-gtag": "5.11.0",
    "gatsby-plugin-react-helmet": "6.11.0",
    "gatsby-plugin-sitemap": "6.11.0",
    "gatsby-plugin-typography": "5.11.0",
    "gatsby-remark-prismjs": "7.11.0",
    "gatsby-source-filesystem": "5.11.0",
    "gatsby-transformer-remark": "6.11.0",
    "prismjs": "1.29.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "react-typography": "0.16.23",
    "typography": "0.16.21",
    "typography-theme-ocean-beach": "0.16.19"
  },
  "devDependencies": {
    "cypress": "12.15.0",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-react": "7.32.2",
    "prettier": "2.8.8",
    "start-server-and-test": "^2.0.0"
  }
}
